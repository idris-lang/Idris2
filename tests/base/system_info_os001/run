rm -rf build

case "$(uname)" in
  Linux*) EXPECTED_OS=unix ;;
  Darwin*) EXPECTED_OS=darwin ;;
  CYGWIN*) EXPECTED_OS=windows ;;
  MINGW*) EXPECTED_OS=windows ;;
  *) echo "unknown uname $(uname)" >&2; exit 1; ;;
esac
export EXPECTED_OS

$1 --no-banner --no-color --console-width 0 Os.idr --exec main

